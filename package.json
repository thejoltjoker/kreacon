{
	"name": "kreacon",
	"version": "0.2.32",
	"private": true,
	"scripts": {
		"postinstall": "npx playwright install --with-deps chromium",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"commitlint": "commitlint --edit",
		"db:dbml": "tsx src/lib/server/db/dbml.ts",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "cross-env DB_MIGRATING=true tsx src/lib/server/db/migrate.ts",
		"db:migrate:prod": "dotenv -e .env.production -- cross-env DB_MIGRATING=true tsx src/lib/server/db/migrate.ts",
		"db:push": "drizzle-kit push",
		"db:reset": "npm run db:migrate && npm run db:seed",
		"db:seed": "cross-env DB_SEEDING=true tsx src/lib/server/db/seed.ts",
		"db:start": "docker compose up",
		"db:studio": "drizzle-kit studio",
		"format": "prettier --write .",
		"lint:fix": "eslint . --fix && prettier --write .",
		"lint": "eslint . && prettier --check .",
		"logbook": "sh logbook.sh ../fsu23d-exjobb-loggbok-thejoltjoker/LOGBOOK.md",
		"prepare": "husky",
		"swa:build": "swa build",
		"swa:deploy": "swa deploy",
		"swa:start": "swa start",
		"test:e2e": "cross-env NODE_ENV=test npm run db:reset && playwright test",
		"test:e2e:ui": "cross-env NODE_ENV=test npm run db:reset && playwright test --ui",
		"test:unit": "cross-env NODE_ENV=test vitest",
		"test": "cross-env NODE_ENV=test npm run db:reset && npm run test:unit -- --run && npm run test:e2e",
		"i18n:translate": "tsx src/lib/i18n/translate.ts",
		"i18n:check": "tsx src/lib/i18n/check.ts",
		"setup:azure": "tsx src/lib/helpers/createContainers.ts"
	},
