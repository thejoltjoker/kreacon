{
	"name": "kreacon",
	"version": "0.0.2",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"db:start": "docker compose up",
		"db:reset": "npm run db:migrate && npm run db:seed",
		"db:push": "drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "cross-env DB_MIGRATING=true tsx src/lib/server/db/migrate.ts",
		"db:seed": "cross-env DB_SEEDING=true tsx src/lib/server/db/seed.ts",
		"db:studio": "drizzle-kit studio",
		"db:dbml": "tsx src/lib/server/db/dbml.ts",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"lint": "eslint . && prettier --check .",
		"lint:fix": "eslint . --fix && prettier --write .",
		"format": "prettier --write .",
		"swa:build": "swa build",
		"swa:start": "swa start",
		"swa:deploy": "swa deploy",
		"prepare": "husky",
		"commitlint": "commitlint --edit",
		"logbook": "sh logbook.sh ../fsu23d-exjobb-loggbok-thejoltjoker/LOGBOOK.md"
	},
