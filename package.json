		"@types/jsonwebtoken": "^9.0.7",
		"@types/throttle-debounce": "^5.0.2",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^1.0.0-next.64",
		"clsx": "^2.1.1",
		"cross-env": "^7.0.3",
		"dotenv-expand": "^12.0.1",
		"drizzle-dbml-generator": "^0.9.0",
		"drizzle-kit": "^0.28.1",
		"eslint": "^9.15.0",
