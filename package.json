{
	"name": "kreacon",
	"version": "0.1.4",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"commitlint": "commitlint --edit",
		"db:dbml": "tsx src/lib/server/db/dbml.ts",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "cross-env DB_MIGRATING=true tsx src/lib/server/db/migrate.ts",
		"db:push": "drizzle-kit push",
		"db:reset": "npm run db:migrate && npm run db:seed",
		"db:seed": "cross-env DB_SEEDING=true tsx src/lib/server/db/seed.ts",
		"db:start": "docker compose up",
		"db:studio": "drizzle-kit studio",
		"format": "prettier --write .",
		"lint:fix": "eslint . --fix && prettier --write .",
		"lint": "eslint . && prettier --check .",
		"logbook": "sh logbook.sh ../fsu23d-exjobb-loggbok-thejoltjoker/LOGBOOK.md",
		"prepare": "husky",
		"swa:build": "swa build",
		"swa:deploy": "swa deploy",
		"swa:start": "swa start",
		"test:e2e": "cross-env NODE_ENV=test npm run db:reset && playwright test",
		"test:e2e:ui": "cross-env NODE_ENV=test npm run db:reset && playwright test --ui",
		"test:unit": "cross-env NODE_ENV=test vitest",
		"test": "cross-env NODE_ENV=test npm run db:reset && npm run test:unit -- --run && npm run test:e2e",
		"translate": "tsx src/lib/i18n/translate.ts"
	},
	"dependencies": {
		"@azure/communication-email": "^1.0.0",
		"@azure/identity": "^4.5.0",
		"@azure/storage-blob": "^12.26.0",
		"@microsoft/applicationinsights-web": "^3.3.4",
		"@sveltejs/adapter-node": "^5.2.10",
		"@sveltejs/kit": "^2.10.1",
		"argon2": "^0.41.1",
		"bits-ui": "^1.0.0-next.64",
		"clsx": "^2.1.1",
		"drizzle-orm": "^0.38.1",
		"http-status-codes": "^2.3.0",
		"lodash": "^4.17.21",
		"lucide-svelte": "^0.468.0",
		"mime": "^4.0.4",
		"postgres": "^3.4.5",
		"svelte": "^5.11.2",
		"svelte-i18n": "^4.0.1",
		"sveltekit-superforms": "^2.21.1",
		"tailwind-merge": "^2.5.5",
		"tailwindcss": "^3.4.16",
		"throttle-debounce": "^5.0.2",
		"zod": "^3.24.1"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.6.0",
		"@commitlint/config-conventional": "^19.6.0",
		"@playwright/test": "^1.49.1",
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/vite-plugin-svelte": "^4.0.1",
		"@tailwindcss/forms": "^0.5.9",
		"@types/eslint": "^9.6.1",
		"@types/jsonwebtoken": "^9.0.7",
		"@types/lodash": "^4.17.13",
		"@types/throttle-debounce": "^5.0.2",
		"autoprefixer": "^10.4.20",
		"cross-env": "^7.0.3",
		"drizzle-dbml-generator": "^0.10.0",
		"drizzle-kit": "^0.30.0",
		"eslint": "^9.16.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.1",
		"eslint-plugin-unused-imports": "^4.1.4",
		"globals": "^15.13.0",
		"husky": "^9.1.7",
		"ollama": "^0.5.11",
		"postcss-nesting": "^13.0.1",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.2",
		"prettier-plugin-tailwindcss": "^0.6.9",
		"svelte-check": "^4.1.1",
		"tsx": "^4.19.2",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.18.0",
		"vite": "^5.4.11",
		"vitest": "^2.1.8"
	},
	"type": "module",
	"engines": {
		"node": "20"
	},
	"optionalDependencies": {
		"@rollup/rollup-linux-x64-gnu": "^4.28.1"
	}
}
